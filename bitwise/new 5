/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */

class Solution {
    public ListNode listDuplication(ListNode head) {
        ListNode answer = head;
        while (head != null){
            ListNode cache = new ListNode(head.val);
            ListNode cacheNext = head.next;
            head.next = cache;
            head = head.next;
            head.next = cacheNext;
            head = head.next;
        }
        return answer;
    }
}
